    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "ng-packagr": "14.0.1",
    "postcss": "^8.3.9",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.3.1",
    "standard-version": "^9.3.1",
    "ts-jest": "27.0.5",
    "typescript": "4.7.3"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
